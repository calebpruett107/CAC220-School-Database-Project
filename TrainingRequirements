-- Create Teacher Training Requirements
CREATE TABLE TrainingRequirements (
    ID INT PRIMARY KEY AUTO_INCREMENT,
    TeacherName VARCHAR(255) NOT NULL,
    TeacherAid BOOLEAN NOT NULL,
    EducationDegree VARCHAR(255) NOT NULL,
    ImmunizationsCleared BOOLEAN NOT NULL
);

