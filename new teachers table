-- create new table called teachers

CREATE TABLE teachers (
    teacher_id INT PRIMARY KEY,
    FirstName VARCHAR(100),
    LastName VARCHAR(100)
);

INSERT INTO teachers (teacher_id, FirstName, LastName) VALUES
